<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.soft.web.dao.GlobalMapper">

    <select id="selectMinePic" resultType="com.soft.web.vo.MinePicVO">
        select mine_pic1,mine_pic2 from fa_global where id=1
    </select>

    <select id="selectWechatSettings" resultType="com.soft.web.vo.GlobalWechatVO">
        select wechat_appid,wechat_merchant_num,wechat_token,wechat_callback_url,wechat_cert_pem,wechat_key_pem from fa_global where id=1
    </select>
</mapper>
